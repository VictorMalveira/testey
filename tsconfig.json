{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/components/botao/Index.jsx", "src/pages/components/button/Index.jsx", "src/pages/components/header/Menu.jsx", "src/pages/components/wellcome/Wellcome.jsx", "src/pages/index.jsx"],
  "exclude": ["node_modules"]
}
